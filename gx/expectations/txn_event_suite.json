      "type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "user_id"
      }
    },
    {
      "type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "merchant_id"
      }
    },
    {
      "type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "device_id"
      }
    },
    {
      "type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "ip_id"
      }
    },
    {
      "type": "expect_column_values_to_not_be_null",
