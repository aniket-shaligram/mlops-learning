      "kwargs": {
        "column": "currency"
      }
    },
    {
      "type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "country"
      }
    },
    {
      "type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "channel"
      }
    },
    {
      "type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "drift_phase"
      }
    },
    {
      "type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "amount",
        "min_value": 0.01,
        "max_value": 100000
      }
    },
    {
      "type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "country",
        "value_set": ["US", "GB", "CA", "DE", "IN", "BR", "SG", "AU", "FR", "JP", "NG"]
      }
    },
    {
      "type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "currency",
        "value_set": ["USD", "GBP", "CAD", "EUR", "INR", "BRL", "SGD", "AUD", "JPY", "NGN"]
      }
    },
    {
      "type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "channel",
        "value_set": ["web", "mobile"]
      }
    },
    {
      "type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "event_type",
        "value_set": ["txn.created"]
      }
    },
    {
      "type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "drift_phase",
        "value_set": [0, 1]
      }
    },
    {
      "type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "user_id",
        "min_value": 0
      }
    },
    {
